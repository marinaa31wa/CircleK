        <script>
            let startTime, elapsed = 0;
            let stopwatchDisplay = document.getElementById('stopwatch');
            let exportButton = document.getElementById('exportButton');
            let intervalId = null;

            function startStopwatch() {
                startTime = Date.now();
                intervalId = setInterval(() => {
                    elapsed = Date.now() - startTime;
                    stopwatchDisplay.textContent = formatElapsedTime(elapsed);
                }, 100);
            }

            function stopStopwatch() {
                if (intervalId) {
                    clearInterval(intervalId);
                    intervalId = null;
                }
            }

            function resetStopwatch() {
                // Reset the elapsed time and update the display
                elapsed = 0;
                stopwatchDisplay.textContent = formatElapsedTime(elapsed);
            }
            resetStopwatch();
            function formatElapsedTime(elapsed) {
                let hours = Math.floor(elapsed / 3600000);
                let minutes = Math.floor((elapsed % 3600000) / 60000);
                let seconds = ((elapsed % 60000) / 1000).toFixed(0);
                return `${pad(hours)}:${pad(minutes)}:${pad(seconds)}`;
            }

            function pad(number) {
                return number < 10 ? '0' + number : number;
            }

            function checkExportStatus() {
                $.ajax({
                    url: '/Home/CheckExportStatus', // Update with your controller and action names
                    type: 'GET',
                    success: function (status) {
                        if (status === 'complete') {
                            // Stop the stopwatch and reset it
                            stopStopwatch();
                            alert('Export to Excel completed');

                            // Call the ResetSessionStatus action to clear the session
                            // $.ajax({
                            //     url: '/Home/ResetSessionStatus', // Update with your controller and action names
                            //     type: 'POST',
                            //     success: function (response) {
                            //         if (response.success) {
                            //             console.log('Session status reset successfully');
                            //         } else {
                            //             console.error('Failed to reset session status');
                            //         }
                            //     },
                            //     error: function () {
                            //         console.error('An error occurred while resetting the session status.');
                            //     }
                            // });
                        } else {
                            // If the export is not yet complete, continue checking
                            setTimeout(checkExportStatus, 1000); // Check every  5 seconds
                        }
                    },
                    error: function () {
                        // Handle errors here
                        stopStopwatch();
                        resetStopwatch();
                        alert('An error occurred while checking the export status.');

                        // Call the ResetSessionStatus action to clear the session
                        $.ajax({
                            url: '/Home/ResetSessionStatus', // Update with your controller and action names
                            type: 'POST',
                            success: function (response) {
                                if (response.success) {
                                    console.log('Session status reset successfully');
                                } else {
                                    console.error('Failed to reset session status');
                                }
                            },
                            error: function () {
                                console.error('An error occurred while resetting the session status.');
                            }
                        });
                    }
                });
            }

            // Event listener for the export button
            exportButton.addEventListener('click', function () {
                // Disable the button to prevent multiple clicks
                // exportButton.disabled = true;

                // Start the stopwatch
                startStopwatch();

                // Trigger the export process here, e.g., by submitting a form or making an AJAX call
                // If you're using AJAX, you can remove the form submission and handle the export in the AJAX success callback
                checkExportStatus(); // Start checking the export status
            });
        </script>

  
        <script>
            document.addEventListener('DOMContentLoaded', function () {
                // Get references to the Quantity and Cost checkboxes
                var ItemLookupCodeCheckbox = document.querySelector('input[name="Parobj.VItemLookupCode"]');
                var ItemNameCheckbox = document.querySelector('input[name="Parobj.VItemName"]');
                var costCheckbox = document.querySelector('input[name="Parobj.VCost"]');
                var priceCheckbox = document.querySelector('input[name="Parobj.VPrice"]');

                // Function to toggle the disabled state of the Cost checkbox
                function toggleCostCheckbox() {
                    costCheckbox.disabled = !(ItemLookupCodeCheckbox.checked || ItemNameCheckbox.checked);
                    priceCheckbox.disabled = !(ItemLookupCodeCheckbox.checked || ItemNameCheckbox.checked);
                }

                // Initially set the Cost checkbox based on the Quantity checkbox
                toggleCostCheckbox();

                // Add event listener to the Quantity checkbox to update the Cost checkbox when its state changes
                ItemLookupCodeCheckbox.addEventListener('change', toggleCostCheckbox);
                ItemNameCheckbox.addEventListener('change', toggleCostCheckbox);
                //ItemNameCheckbox.addEventListener('change', toggleCostCheckbox);
            });
